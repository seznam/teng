#ifndef TENGVERSION_H
#define TENGVERSION_H

#include <stdint.h>

namespace Teng {

/** Teng library major version (libtool's CURRENT version component).
 */
#define TENG_MAJOR @TENG_MAJOR@

/** Teng library minor version (libtool's REVISION version component).
 */
#define TENG_MINOR @TENG_MINOR@

/** Teng library version.
 */
#define TENG_VERSION "@TENG_MAJOR@.@TENG_MINOR@"

/** Integral type used inside teng
 */
typedef intmax_t IntType_t;

} // namespace Teng

#endif // TENGVERSION_H
